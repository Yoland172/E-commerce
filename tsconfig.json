{
    "compilerOptions": {
      "esModuleInterop": true,
      "jsx": "react",
      "module": "esnext",
      "moduleResolution": "node",
      "plugins": [{ "name": "typescript-plugin-css-modules" }],
      "lib": [
        "dom",
        "esnext"
      ],
      "strict": true,
      "sourceMap": true,
      "target": "esnext",
      "resolveJsonModule": true
    },
    "exclude": [
      "node_modules"
    ]
}
